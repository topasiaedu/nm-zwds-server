{
  "name": "nm-zwds-server",
  "version": "1.0.0",
  "description": "A simple Node.js TypeScript server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "render-build": "npm run build",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@types/node": "^20.19.10",
    "@types/pdfkit": "^0.17.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pdfkit": "^0.17.1",
    "puppeteer": "^24.16.0",
    "solarlunar": "^2.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
