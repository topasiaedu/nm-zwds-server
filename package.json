{
  "name": "nm-zwds-server",
  "version": "1.0.0",
  "description": "A simple Node.js TypeScript server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias && npm run copy-assets",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "render-build": "npm ci && npm run build",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "copy-assets": "node -e \"const fs=require('fs'),path=require('path');const copy=(s,d)=>{fs.mkdirSync(d,{recursive:true});for(const e of fs.readdirSync(s,{withFileTypes:true})){const sp=path.join(s,e.name),dp=path.join(d,e.name);e.isDirectory()?copy(sp,dp):fs.copyFileSync(sp,dp);}};copy('src/assets','dist/assets');\""
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.10",
    "@types/pdfkit": "^0.17.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pdfkit": "^0.17.1",
    "puppeteer": "^24.16.0",
    "solarlunar": "^2.0.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
