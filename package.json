{
  "name": "nm-zwds-server",
  "version": "1.0.0",
  "description": "A simple Node.js TypeScript server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.2",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.12",
    "googleapis": "^155.0.0",
    "helmet": "^7.1.0",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "puppeteer": "^24.16.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "solarlunar": "^2.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
